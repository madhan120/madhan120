l=list(map(int,input().split(',')))
indexs=list(map(int,input().split(',')))
values=list(map(int,input().split(',')))
s=[]
for i in range(len(indexs)):
    l[indexs[i]]=l[indexs[i]]+values[i]
    sum1=0
    for j in l:
        if j%2==0:
            sum1=sum1+j
    s.append(sum1)    
                
print(s)        
